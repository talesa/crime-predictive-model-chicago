    "def generate_predictions_for_all_bins(temp, prcp):\n",
    "    df = pd.DataFrame()\n",
    "    df['bin'] = np.arange(no*no)\n",
    "    \n",
    "    df['count'] = df['bin'].map(lambda x: \n",
    "        predict_for_bin(\n",
    "            get_counts_by_date_for_bin(x),\n",
    "            generate_nextday(temp, prcp)\n",
    "        )\n",
    "    )\n",
    "    \n",
    "    return df"
