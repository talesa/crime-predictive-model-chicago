    "    \n",
    "    bin_crime_data = add_dummies(bin_crime_data)\n",
    "    \n",
    "    #TODO change\n",
    "    endog = bin_crime_data.loc[:, ['count']]\n",
    "    exog = bin_crime_data.loc[:, 'PRCP':]\n",
    "    exog = exog.drop(['AWND','weekday','yearday'], axis=1)\n",
    "    \n",
    "    prediction = fit_sm_predict_row(endog, exog, next_day_row)\n",
    "    \n",
    "    return prediction[0]"
